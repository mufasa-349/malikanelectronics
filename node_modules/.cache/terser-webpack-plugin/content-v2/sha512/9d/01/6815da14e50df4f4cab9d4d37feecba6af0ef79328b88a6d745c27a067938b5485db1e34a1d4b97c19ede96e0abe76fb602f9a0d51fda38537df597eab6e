x) => (\n                        <div\n                            key={slide.id}\n                            className={`slide ${index === currentSlide ? 'active' : ''}`}\n                            style={{\n                                backgroundImage: `url(${slide.image})`,\n                                backgroundSize: 'cover',\n                                backgroundPosition: 'center',\n                                backgroundRepeat: 'no-repeat'\n                            }}\n                        >\n                            {slide.external ? (\n                                <a \n                                    href={slide.link} \n                                    target=\"_blank\" \n                                    rel=\"noopener noreferrer\"\n                                    className=\"slide-link\"\n                                    style={{\n                                        display: 'block',\n                                        width: '100%',\n                                        height: '100%',\n                                        cursor: 'pointer',\n                                        position: 'absolute',\n                                        top: 0,\n                                        left: 0,\n                                        zIndex: 2\n                                    }}\n                                >\n                                </a>\n                            ) : (\n                                <Link \n                                    to={slide.link} \n                                    className=\"slide-link\"\n                                    style={{\n                                        display: 'block',\n                                        width: '100%',\n                                        height: '100%',\n                                        cursor: 'pointer',\n                                        position: 'absolute',\n           