found) return found\n            }\n        }\n        return null\n    }\n    return searchInCategories(CategoriesData)\n}\n\n// Ana kategorileri getir\nexport const getMainCategories = () => {\n    return getMainCategoriesData()\n}\n\n// Kategori breadcrumb olutur\nexport const getCategoryBreadcrumb = (slug) => {\n    const breadcrumb = []\n    \n    const findPath = (categories, targetSlug, path = []) => {\n        for (const category of categories) {\n            const currentPath = [...path, category]\n            \n            if (category.slug === targetSlug) {\n                return currentPath\n            }\n            \n            if (category.children) {\n                const found = findPath(category.children, targetSlug, currentPath)\n                if (found) return found\n            }\n        }\n        return null\n    }\n    \n    const path = findPath(CategoriesData, slug)\n    return path || []\n}\n","import React, { useState, useEffect } from 'react'\nimport { useParams, useLocation } from 'react-router-dom'\nimport ProductCard from '../Common/Product/ProductCard'\nimport Filter from './Filter'\nimport { useSelector } from \"react-redux\"\nimport { findCategoryBySlug, getCategoryBreadcrumb } from '../../app/data/categoriesData'\nimport { getProductsByCategory } from '../../app/data/productsData'\n\nconst CategoryShop = () => {\n    const { categorySlug } = useParams()\n    const location = useLocation()\n    const [products, setProducts] = useState([])\n    const [page, setPage] = useState(1)\n    const [category, setCategory] = useState(null)\n    const [breadcrumb, setBreadcrumb] = useState([])\n    const [loading, setLoading] = useState(true)\n    const [filtering, setFiltering] = useState(false)\n    const [categoryChanging, setCategoryChanging] = useState(false)\n    const [previousCategory, setPreviousCategory] = useState(null)\n    \n    // Tm rnleri al\n    let allProducts = useSelector((state) => state.products.products)\n    \n    useEffect(() => {\n        setLoading(true)\n        setCategoryChanging(true)\n        \n        // URL'den kategori slug'n al\n        const urlParams = new URLSearchParams(location.search)\n        const categoryParam = urlParams.get('category') || categorySlug\n        \n        // nceki kategoriyi kaydet\n        if (category && category.slug !== categoryParam) {\n            setPreviousCategory(category)\n        }\n        \n        // Simle edilmi loading delay\n        setTimeout(() => {\n            if (categoryParam) {\n                // Kategoriyi bul\n                const foundCategory = findCategoryBySlug(categoryParam)\n                setCategory(foundCategory)\n                \n                // Breadcrumb olutur\n                const breadcrumbPath = getCategoryBreadcrumb(categoryParam)\n                setBreadcrumb(breadcrumbPath)\n                \n                // Kategoriye gre rnleri filtrele\n                filterProductsByCategory(categoryParam)\n            } else {\n                // Tm rnleri gster\n                setProducts(allProducts)\n                setCategory({ name: \"Tm rnler\", slug: \"all\" })\n            }\n            setLoading(false)\n            \n            // Kategori deiiklii animasyonunu bitir\n            setTimeout(() => {\n                setCategoryChanging(false)\n                setPreviousCategory(null)\n            }, 300)\n        }, 500)\n    }, [categorySlug, location.search, allProducts])\n    \n    const filterProductsByCategory = (categorySlug) => {\n        // Gerek rn verilerini kullan\n        const filteredProducts = getProductsByCategory(categorySlug)\n        setProducts(filteredProducts)\n    }\n    \n    const randProduct = (page) => {\n        if (page) {\n            setFiltering(true)\n            setTimeout(() => {\n                let data = [...products].sort((a, b) => 0.5 - Math.random())\n                setProducts(data)\n                setPage(page)\n                setFiltering(false)\n            }, 300)\n        }\n    }\n    \n    return (\n        <>\n            <section id=\"shop_main_area\" className=\"ptb-100\">\n                <div className=\"container\">\n                    {/* Breadcrumb */}\n                    {breadcrumb.length > 0 && (\n                        <nav aria-label=\"breadcrumb\" className={`mb-4 ${categoryChanging ? 'updating' : ''}`}>\n                            <ol className=\"breadcrumb\">\n                                <li className=\"breadcrumb-item\">\n                                    <a href=\"/\">Ana Sayfa</a>\n                                </li>\n                                {breadcrumb.map((item, index) => (\n                                    <li key={index} className={`breadcrumb-item ${index === breadcrumb.length - 1 ? 'active' : ''}`}>\n                                        {index === breadcrumb.length - 1 ? (\n                                            item.name\n                                        ) : (\n                                            <a href={`/shop?category=${item.slug}`}>{item.name}</a>\n                                        )}\n                                    </li>\n                                ))}\n                            </ol>\n                        </nav>\n                    )}\n                    \n                    {/* Kategori Bal */}\n                    <div className={`category-header mb-4 ${categoryChanging ? 'changing' : ''}`}>\n                        <h1 className={`category-title ${categoryChanging ? 'changing' : ''}`}>\n                            {category ? category.name : 'rnler'}\n                        </h1>\n                        <p className={`category-description ${categoryChanging ? 'updating' : ''}`}>\n                            {category ? `${category.name} kategorisindeki ${products.length} rn` : `${products.length} rn bulundu`}\n                        </p>\n                        {categoryChanging && previousCategory && (\n                            <div className=\"category-transition-indicator\">\n                                <small className=\"text-muted\">\n                                    {previousCategory.name}  {category ? category.name : 'Tm rnler'}\n                                </small>\n                            </div>\n                        )}\n                    </div>\n                    \n                    <Filter filterEvent={randProduct}/>\n                    \n                    {/* Loading State */}\n                    {loading && (\n                        <div className=\"loading-container text-center py-5\">\n                            <div className=\"loading-spinner\">\n                                <div className=\"spinner-border text-primary\" role=\"status\">\n                                    <span className=\"sr-only\">Ykleniyor...</span>\n                                </div>\n                            </div>\n                            <p className=\"loading-text mt-3\">rnler ykleniyor...</p>\n                        </div>\n                    )}\n                    \n                    {/* Products Grid */}\n                    <div className={`products-grid ${loading ? 'loading' : ''} ${filtering ? 'filtering' : ''}`}>\n                        <div className=\"row\">\n                            {!loading && products.length > 0 ? (\n                                products.map((data, index) => (\n                                    <div className=\"col-lg-3 col-md-4 col-sm-6 col-12 product-item\" key={index}>\n                                        <ProductCard data={data} />\n                                    </div>\n                                ))\n                            ) : !loading && products.length === 0 ? (\n                                <div className=\"col-12\">\n                                    <div className=\"no-products text-center py-5\">\n                                        <h3>Bu kategoride rn bulunamad</h3>\n                                        <p>Farkl bir kategori semeyi deneyin.</p>\n                                    </div>\n                                </div>\n                            ) : null}\n                        </div>\n                    </div>\n                    \n                    {/* Pagination */}\n                    {!loading && products.length > 0 && (\n                        <div className=\"col-lg-12\">\n                            <ul className=\"pagination\">\n                                <li className=\"page-item\" onClick={(e) => { randProduct(page > 1 ? page - 1 : 0) }}>\n                                    <a className=\"page-link\" href=\"#!\" aria-label=\"Previous\">\n                                        <span aria-hidden=\"true\"></span>\n                                    </a>\n                                </li>\n                                <li className={\"page-item \" + (page === 1 ? \"active\" : null)} onClick={(e) => { randProduct(1) }}>\n                                    <a className=\"page-link\" href=\"#!\">1</a>\n                                </li>\n                                <li className={\"page-item \" + (page === 2 ? \"active\" : null)} onClick={(e) => { randProduct(2) }}>\n                                    <a className=\"page-link\" href=\"#!\">2</a>\n                                </li>\n                                <li className={\"page-item \" + (page === 3 ? \"active\" : null)} onClick={(e) => { randProduct(3) }}>\n                                    <a className=\"page-link\" href=\"#!\">3</a>\n                                </li>\n                                <li className=\"page-item\" onClick={(e) => { randProduct(page < 3 ? page + 1 : 0) }}>\n                                    <a className=\"page-link\" href=\"#!\" aria-label=\"Next\">\n                                        <span aria-hidden=\"true\"></span>\n                                    </a>\n                                </li>\n                            </ul>\n                        </div>\n                    )}\n                </div>\n            </section>\n        </>\n    )\n}\n\nexport default CategoryShop\n","import React, { useEffect, useState } from 'react'\nimport { useParams, useLocation } from 'react-router-dom'\nimport Header from '../../component/Common/Header'\nimport Banner from '../../component/Common/Banner'\nimport CategoryShop from '../../component/Shop/CategoryShop'\nimport InstgramSlider from '../../component/Common/Instagram'\nimport Footer from '../../component/Common/Footer'\nimport { findCategoryBySlug } from '../../app/data/categoriesData'\n\nconst CategoryPage = () => {\n    const { categorySlug } = useParams()\n    const location = useLocation()\n    const [pageTitle, setPageTitle] = useState(\"Kategori rnleri\")\n    const [pageChanging, setPageChanging] = useState(false)\n    \n    useEffect(() => {\n        setPageChanging(true)\n        \n        // URL'den kategori slug'n al\n        const urlParams = new URLSearchParams(location.search)\n        const categoryParam = urlParams.get('category') || categorySlug\n        \n        if (categoryParam) {\n            const category = findCategoryBySlug(categoryParam)\n            if (category) {\n                setPageTitle(`${category.name} rnleri`)\n            } else {\n                setPageTitle(\"Kategori rnleri\")\n            }\n        } else {\n            setPageTitle(\"Tm rnler\")\n        }\n        \n        // Sayfa deiiklii animasyonu\n        setTimeout(() => {\n            setPageChanging(false)\n        }, 300)\n    }, [categorySlug, location.search])\n    \n    return (\n        <>\n            <Header />\n            <div className={`page-transition ${pageChanging ? 'changing' : ''}`}>\n                <Banner title={pageTitle} />\n                <CategoryShop />\n                <InstgramSlider />\n            </div>\n            <Footer />\n        </>\n    )\n}\n\nexport default CategoryPage\n","import React from 'react'\nimport { Link } from 'react-router-dom';\nconst Banner = (props) => {\n    return (\n        <>\n            <section id=\"common_banner_one\">\n                <div className=\"container \">\n                    <div className=\"row\">\n                        <div className=\"col-lg-12\">\n                            <div className=\"common_banner_text\">\n                                <h2>{props.title}</h2>\n                                <ul>\n                                    <li><Link to=\"/\">Home</Link></li>\n                                    <li className=\"slash\">/</li>\n                                    <li className=\"active\">{props.title}</li>\n                                </ul>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </section>\n        </>\n    )\n}\n\nexport default Banner","import React from 'react'\nimport { Link } from 'react-router-dom'\nconst Filter = (props) => {\n    return (\n        <>\n            <div className=\"row\">\n                <div className=\"col-lg-6 col-md-12\">\n                    <div className=\"product_filter\">\n                        <div className=\"customs_selects\">\n                            <select name=\"product\" className=\"customs_sel_box\" onChange={() => { props.filterEvent(1) }}>\n                                <option value=\"volvo\">Most Popular</option>\n                                <option value=\"saab\">Best Seller</option>\n                                <option value=\"mercedes\">Trending</option>\n                                <option value=\"audi\">Featured</option>\n                            </select>\n                        </div>\n                    </div>\n                </div>\n                <div className=\"col-lg-6 col-md-12\">\n                    <div className=\"product_shot\">\n                        <div className=\"product_shot_title\">\n                            <p>Sort By:</p>\n                        </div>\n                        <div className=\"customs_selects\">\n                            <select name=\"product\" className=\"customs_sel_box\" onChange={() => { props.filterEvent(1) }}>\n                                <option value=\"Popularity\">Sort By Popularity</option>\n                                <option value=\"newness\">Sort By Newness</option>\n                                <option value=\"low\">Price: Low to High</option>\n                                <option value=\"high\">Price: High to Low</option>\n                            </select>\n                        </div>\n                        <div className=\"product_shot_view\">\n                            <ul>\n                                <li><Link to=\"/shopList\"><i className=\"fa fa-list\"></i></Link></li>\n                                <li><Link to=\"/shopTwo\"><i className=\"fa fa-th-large\"></i></Link></li>\n                                <li><Link to=\"/shop\"><i className=\"fa fa-th\"></i></Link></li>\n                            </ul>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </>\n    )\n}\n\nexport default Filter\n"]}}